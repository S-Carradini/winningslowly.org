{
  "files.associations": {
    "*.html": "jinja-html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "docs": true,
    "__pycache__": true,
    ".idea": true,
    ".sass-cache": true,
    "cache": true,
    "recordings": true,
    "resources": true,
    "research": true
  },
  "editor.rulers": [
    90
  ],
  "[scss]": {
    "editor.formatOnSave": true
  },
  "prettier.printWidth": 90
}
